        "gpxparser": "^3.0.8",
        "hamburger-react": "^2.4.1",
        "jspdf": "^2.4.0",
        "md5": "^2.3.0",
        "ol": "^6.9.0",
        "ol-ext": "^3.2.6",
        "papaparse": "^5.3.1",
        "query-string": "^7.0.1",
        "react": "^17.0.2",
        "react-bootstrap": "^2.0.0-beta.4",
        "react-dom": "^17.0.2",
